{
       "env": {
           "browser": true,
           "es2021": true
       },
       "extends": [
           "eslint:recommended",
           "plugin:react/recommended",
           "plugin:react-hooks/recommended",
           "plugin:prettier/recommended"
       ],
       "parserOptions": {
           "ecmaVersion": 12,
           "sourceType": "module",
           "ecmaFeatures": {
               "jsx": true
           }
       },
       "rules": {
           "prettier/prettier": "error",
           "react/prop-types": "off"
       }
   }